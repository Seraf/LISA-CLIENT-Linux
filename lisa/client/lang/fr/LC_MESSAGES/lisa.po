#: ../lib/speaker.py:29
msgid "Yes ?"
msgstr "Oui ?"

#: ../lib/speaker.py:30
msgid "I'm listening"
msgstr "Je vous écoute"

#: ../lib/speaker.py:32
msgid "My configuration file is not readable"
msgstr "Mon fichier de configuration est erronné"

#: ../lib/speaker.py:33
msgid "There's an error with my configuration file"
msgstr "J'ai détecté une erreur dans mon fichier de configuration"

#: ../lib/speaker.py:35
msgid "I didn't understood your question"
msgstr "Je n'ai pas compris votre question"

#: ../lib/speaker.py:36
msgid "I can't hear you clearly"
msgstr "Je ne vous entends pas bien"

#: ../lib/speaker.py:37
msgid "I didn't understood"
msgstr "Je n'ai pas compris"

#: ../lib/speaker.py:39
msgid "Can you repeat please ?"
msgstr "Pouvez vous répeter s'il vous plait ?"

#: ../lib/speaker.py:40
msgid "I didn't understood, can you repeat please ?"
msgstr "Je n'ai pas compris, pouvez vous répéter ?"

#: ../lib/speaker.py:42
msgid "I'm ready ok"
msgstr "Je suis prêt"

#: ../lib/speaker.py:43
msgid "Initialization completed"
msgstr "Mon initialisation est terminée"

#: ../lib/speaker.py:44
msgid "I'm ready to answer your questions"
msgstr "Je suis prêt à répondre à vos questions"

#: ../lib/speaker.py:46
msgid "Sorry, I can't connect to the server"
msgstr "Désolé je n'arrive pas a me connecter au serveur"

#: ../lib/speaker.py:47
msgid "I can't join the server, please check your connection"
msgstr "Le serveur est introuvable, veuillez vérifier la connexion"

#: ../lib/speaker.py:49
msgid "An error happened, I'm not available anymore"
msgstr "Il s'est produit une erreur, je ne suis plus disponible"

#: ../lib/speaker.py:50
msgid "My connection was interrupted, please wait"
msgstr "Ma connexion au serveur est interrompue, veuillez patienter"

#: ../lib/speaker.py:167
msgid "Playing generated TTS"
msgstr "Lecture du son généré"

#: ../lib/speaker.py:170
#, python-format
msgid "There was an error creating the output file %(filename)s"
msgstr "Il y a eu une lors de la création du fichier %(filename)s"

#: ../lib/speaker.py:187
#, python-format
msgid "Generating %(filename)s : '%(message)s'"
msgstr "Création du fichier %(filename)s : '%(message)s'"
